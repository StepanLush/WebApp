global:
  scrape_interval: 15s

scrape_configs:
  - job_name: 'prometheus'
    static_configs:
      - targets: ['localhost:9090']

  - job_name: 'frontend'
    static_configs:
      - targets: ['{{ frontend_ip }}:9113']

  - job_name: 'backend'
    static_configs:
      - targets: ['{{ backend_ip1 }}:3000', '{{ backend_ip2 }}:3000']