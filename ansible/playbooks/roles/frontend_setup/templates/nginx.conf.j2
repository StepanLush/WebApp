server {
    listen 80;
    server_name _;

    root {{ frontend_app_path }};
    index index.html;

    location / {
        try_files $uri /index.html;
    }

}

server {
    listen 8080;
    server_name localhost;

    location /stub_status {
        stub_status on;
        access_log off;
        allow 127.0.0.1;
        deny all;
    }
}